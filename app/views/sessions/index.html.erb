<h1>Welcome to Acebook</h1>

<%= form_for :user, url: users_path do |f| %>
  First Name: <%= f.text_field :first_name, name: "first_name", value: @first_name %><br />
  Last Name: <%= f.text_field :last_name, name: "last_name", value: @last_name %><br />
  Email: <%= f.text_field :email, name: "email", value: @email %><br />
  Password: <%= f.password_field :password, name: "password" %><br />
  Confirm Password: <%= f.password_field :password_confirmation, name: "password_confirmation" %><br />
  <%= f.submit :Submit, name: "signup" %><br />
<% end %>

<p><%= @validation_message %></p>